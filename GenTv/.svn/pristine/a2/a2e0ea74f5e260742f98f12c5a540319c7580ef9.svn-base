package gen.entertainments.gentv.utils;

import android.util.Log;

public class Logger {
	public static boolean isDebug = true;
	
	public static void d(String tag, String msg) {
		if(isDebug) {
			Log.d("stk", msg);
		}
	}
	
	public static void e(String msg) {
		if(isDebug) {
			Log.e("stk", msg);
		}
	}	
	
}
